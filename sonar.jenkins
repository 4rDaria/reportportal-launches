pipeline {
    agent any

    options {
      buildDiscarder(logRotator(numToKeepsStr: '5'))
    }

    stages {
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sq1') {
                  bat '/mvnw clean org.sonarsourse.scanner.maven:sonar-maven-plugin:4.0.0.4121:sonar'
                }
            }
        }
    }
}